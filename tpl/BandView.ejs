<%
%>
<div id="pic" class="text-align-center profile_pic">
    <img src="<%= logo %>" alt="img">
    <div class="name"><%= name %></div>
    <div class="row">
        <span class="col-50">User score: 5.8</span>
        <span class="col-50">LNO score: 8.5</span>
    </div>
    <div class="row">
        <a href="<%= website %>">Website</a> &nbsp;
        <a href="<%= youtube %>">Youtube</a>
        <a href="<%= twitter %>">Twitter</a>
        <a href="<%= instagram %>">Instagram</a>
    </div>
</div>
<div id="venues" class="list-block media-list">
    <ul>
        <li data-id="<%= id %>">
            Venues will be listed here
        </li>
    </ul>
</div>