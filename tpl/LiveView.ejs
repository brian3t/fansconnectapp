<%
let bands = collections.bands;
%>
<div class="list media-list searchbar-found">
    <ul>
        <% bands.forEach((band) => {
            let {id, name, logo, lno_score, genre} = band.attributes
            let star_html = band.pull_star_html()
            let venue = band.pull_random_venue();
            console.log(venue);
            if (genre == undefined) {
                genre = '';
            }
            console.log(genre.ucwords())
        %>
        <li data-id="<%= id %>">
            <a href="#" class="item-link item-content">
                <div class="item-media"><img src="<%= band.get('logo') %>" width="80"/></div>
                <div class="item-inner">
                    <div class="item-title-row row">
                        <div class="item-title col-75"><%= name %></div>
                        <div class="col-25 font-italic"><span class="geolocate distance" data-lat="<%= venue.get('lat') %>" data-lng="<%= venue.get('lng') %>">0</span> (mile)</div>
                    </div>
                    <div class="row no-pad">
		  <span class="col-75 rating"><%= star_html %> &nbsp; Genre: <%= genre.ucwords().replace(/,/g, ', ') %>
              <br/><span class="font-italic">Mon-Sep 22nd 9:00PM</span>
		  </span>
                        <span class="col-25 font-italic">$$$</span>
                    </div>
                </div>
            </a>
        </li>
        <% }) %>
    </ul>
</div>
